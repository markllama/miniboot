#!ipxe

set BASEURL=http://{{ server.addr }}

kernel ${BASEURL}/coreos/kernel initrd=main coreos.live.rootfs_url=${BASEURL}/coreos/rootfs ignition.firstboot ignition.platform.id=metal ignition.config.url=${BASEURL}/config.ign systemd.unified_cgroup_hierarchy=0
initrd --name main ${BASEURL}/coreos/initramfs

boot